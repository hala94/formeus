{
  "name": "@formeus/svelte",
  "version": "1.0.1",
  "description": "Input state and validation manager library for Svelte",
  "license": "MIT",
  "keywords": [
    "form",
    "form validation",
    "svelte form"
  ],
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    }
  },
  "scripts": {
    "build": "rm -rf lib && rollup -c",
    "dev": "rollup -c --watch",
    "clean": "rm -rf lib"
  },
  "dependencies": {
    "@formeus/core": "workspace:*"
  },
  "devDependencies": {
    "eslint-config-shared": "workspace:*",
    "svelte": "^3.55.1",
    "tsconfig": "workspace:*"
  },
  "browser": {},
  "typesVersions": {},
  "peerDependencies": {
    "svelte": "^3.55.1"
  },
  "files": [
    "lib/*"
  ]
}
