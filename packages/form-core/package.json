{
  "name": "@formeus/core",
  "version": "1.1.0",
  "description": "Input state and validation manager library for TS/JS",
  "license": "MIT",
  "keywords": [
    "form",
    "form validation"
  ],
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    }
  },
  "scripts": {
    "build": "rm -rf lib && rollup -c",
    "dev": "rollup -c --watch",
    "test": "vitest --run",
    "lint": "eslint src/*",
    "clean": "rm -rf lib"
  },
  "devDependencies": {
    "eslint-config-shared": "workspace:*",
    "tsconfig": "workspace:*",
    "vite": "^4.0.4",
    "vitest": "^0.27.0"
  },
  "files": [
    "lib/*"
  ]
}
